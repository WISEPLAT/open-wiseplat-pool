{{#if model.immature}}
<h4>Неподтвержденные блоки</h4>

<div class="table-responsive">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
		  <th>Номер</th>
		  <th>Хэш блока</th>
		  <th>Время генерации</th>
		  <th>Удача</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.immature as |block|}}
        {{partial "blocks/block"}}
      {{/each}}
    </tbody>
  </table>
</div>
{{else}}
<h3>Пока нет неподтвержденных блоков</h3>
{{/if}}
